window winword /WINWORD.EXE*/ : Global
 if ( USE104 )
   key C-Equal = C-d b Enter
   key C-S-Equal = C-d p Enter
 else
   key C-Hyphen = C-t b Enter
   key C-S-Hyphen = C-t p Enter
 endif
# }}}

